<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ar.com.templateit.cds.web.entity.UsuarioEcommerce" table="usuarioecommerce">
        <id name="id" type="java.lang.Long">
            <column name="ID" />
            <generator class="identity" />
        </id>
		
        <property name="apellido" type="string">
            <column name="APELLIDO" length="80" not-null="true" />
        </property>
        <property name="nombres" type="string">
            <column name="NOMBRES" length="100" not-null="true" />
        </property>
        <property name="dni" type="long">
            <column name="DNI" not-null="true" />
        </property>
        <property name="direccion" type="string">
            <column name="DIRECCION" length="150" not-null="true" />
        </property>
        <property name="telefono" type="string">
            <column name="TELEFONO" length="25" not-null="true" />
        </property>
        <property name="email" type="string">
            <column name="EMAIL" length="100" not-null="true" />
        </property>
        <property name="usuario" type="string">
            <column name="USUARIO" length="50" not-null="true" />
        </property>
        <property name="password" type="string">
            <column name="PASSWORD" length="60" not-null="true" />
        </property>
                
    </class>
</hibernate-mapping>

